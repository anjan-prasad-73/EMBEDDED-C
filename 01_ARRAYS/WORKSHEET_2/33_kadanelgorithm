#include <stdio.h>

int main() {
    int arr[5]={1,2,-1,3,4};
    int n=sizeof(arr)/sizeof(arr[0]);
    int  i, maxCurrent, maxGlobal;

    maxCurrent = maxGlobal = arr[0];
    for(i = 1; i < n; i++){
        maxCurrent = (arr[i] > maxCurrent + arr[i]) ? arr[i] : maxCurrent + arr[i];
        if(maxCurrent > maxGlobal) maxGlobal = maxCurrent;
    }

    printf("Maximum sum of subarray%d\n", maxGlobal);
    return 0;
}
